   "source": [
    "from PIL import Image\n",
    "from IPython.display import display, clear_output\n",
    "import random\n",
    "import requests\n",
    "import time\n",
    "import json\n",
    "\n",
    "def send_to_google_form(data_dict, form_url):\n",
    "    form_id_start = form_url.find('/e/') + 3\n",
    "    form_id_end = form_url.find('/viewform', form_id_start)\n",
    "    form_id = form_url[form_id_start:form_id_end]\n",
    "\n",
    "    \n",
    "    view_form_url = f'https://docs.google.com/forms/d/e/{form_id}/viewform'\n",
    "    post_form_url = f'https://docs.google.com/forms/d/e/{form_id}/formResponse'\n",
    "\n",
    "    page = requests.get(view_form_url)\n",
    "    content = page.text\n",
    "\n",
    "    # Print the content for debugging purposes\n",
    "    print(f\"Content from the view form URL:\\n{content}\")\n",
    "\n",
    "    loaded_all = True\n",
    "\n",
    "    try:\n",
    "        if not content:\n",
    "            print(\"Content is empty. Data not uploaded.\")\n",
    "            return False\n",
    "\n",
    "        result_start = content.find('FB_PUBLIC_LOAD_DATA_') + 20\n",
    "        result_end = content.find(');</script>', result_start)\n",
    "        result = json.loads(content[result_start:result_end])\n",
    "        \n",
    "        form_dict = {}\n",
    "        for item in result:\n",
    "            if item[1] not in data_dict:\n",
    "                print(f\"Form item {item[1]} not found. Data not uploaded.\")\n",
    "                loaded_all = False\n",
    "                return False\n",
    "            form_dict[f'entry.{item[4][0][0]}'] = data_dict[item[1]]\n",
    "        \n",
    "        post_result = requests.post(post_form_url, data=form_dict)\n",
    "        return post_result.ok\n",
    "    except (json.JSONDecodeError, ValueError) as e:\n",
    "        print(f\"Error decoding JSON or accessing result: {e}\")\n",
    "        return False\n",
    "        \n",
    "class ImageDisplay:\n",
    "    def __init__(self, image_path, duration, callback):\n",
    "        self.img = Image.open(image_path).convert('RGB')\n",
    "        display(self.img)\n",
    "\n",
    "        time.sleep(duration)\n",
    "        clear_output(wait=True)\n",
    "        print(\"Press 'Continue' to proceed.\")\n",
    "        self.continue_button = input(\"Continue\")\n",
    "        clear_output(wait=True)\n",
    "\n",
    "        callback()\n",
    "\n",
    "def get_user_name():\n",
    "    user_name = input(\"Hello! What is your name?\")\n",
    "    return user_name\n",
    "\n",
    "def ask_question(question, answer):\n",
    "    user_input = input(question)\n",
    "\n",
    "    if user_input and user_input.lower() == str(answer).lower():\n",
    "        print(\"Great!\")\n",
    "        return True\n",
    "    else:\n",
    "        print(\"Incorrect!\")\n",
    "        return False\n",
    "\n",
    "def data_consent():\n",
    "    data_consent_info = \"\"\"DATA CONSENT INFORMATION:\n",
    "\n",
    "Please read:\n",
    "\n",
    "We wish to record your response data to an anonymized public data repository.\n",
    "\n",
    "Your data will be used for educational teaching purposes practicing data analysis and visualization.\n",
    "\n",
    "Please type 'yes' in the box below if you consent to the upload.\"\"\"\n",
    "\n",
    "    print(data_consent_info)\n",
    "    result = input(\"> \")\n",
    "\n",
    "    if result.lower() == \"yes\":\n",
    "        print(\"Thanks for your participation.\")\n",
    "        print(\"Please contact philip.lewis@ucl.ac.uk if you have any questions or concerns regarding the stored results\")\n",
    "    else:\n",
    "        raise Exception(\"User did not consent to continue the test.\")\n",
    "\n",
    "def generate_user_id():\n",
    "    id_instructions = \"\"\"\n",
    "\n",
    "Enter your anonymized ID\n",
    "\n",
    "To generate an anonymous 4-letter unique user identifier, please enter:\n",
    "- two letters based on the initials (first and last name) of a childhood friend\n",
    "- two letters based on the initials (first and last name) of a favorite actor / actress\n",
    "\n",
    "e.g., if your friend was called Charlie Brown and the film star was Tom Cruise,\n",
    "then your unique identifier would be CBTC\n",
    "\"\"\"\n",
    "    print(id_instructions)\n",
    "    user_id = input(\"> \")\n",
    "    print(\"User entered id:\", user_id)\n",
    "    return user_id\n",
    "\n",
    "def memory_test():\n",
    "    random.seed(1)\n",
    "    data_consent()\n",
    "    user_id = generate_user_id()\n",
    "    user_name = get_user_name()\n",
    "\n",
    "    introduction = f\"{user_name}, you will be shown a grid of 9 different shapes of 9 colors. Try to remember them, and then you will be asked a series of questions to test your memory.\"\n",
    "    print(introduction)\n",
    "\n",
    "    image_path = \"shapes grid.png\"\n",
    "    data_dict = {\n",
    "        \"Name\": user_name,\n",
    "        \"ID\": user_id,\n",
    "        \"Age\": None,\n",
    "        \"Gender\": None,\n",
    "        \"Score\": None,\n",
    "    }\n",
    "\n",
    "    def after_image_disappears():\n",
    "        ask_questions(user_name, user_id, data_dict)\n",
    "\n",
    "    display_obj = ImageDisplay(image_path, 10, after_image_disappears)\n",
    "\n",
    "def ask_questions(user_name, user_id, data_dict):\n",
    "    questions = [\n",
    "        (\"Which shape was pink?\", \"star\"),\n",
    "        (\"What color was the triangle?\", \"green\"),\n",
    "        (\"Which row was the pentagon in? (1, 2, or 3)\", \"2\"),\n",
    "        (\"What color was the hexagon?\", \"black\"),\n",
    "        (\"Was there an orange shape? (yes or no)\", \"no\")]\n",
    "\n",
    "    score = 0\n",
    "\n",
    "    for question, answer in questions:\n",
    "        if ask_question(question, answer):\n",
    "            score += 1\n",
    "\n",
    "    print(f\"Your Score: {score} out of {len(questions)}. Please answer the following short questions about yourself, so we can record your data\")\n",
    "\n",
    "    data_dict[\"Age\"] = int(input(\"How old are you?\"))\n",
    "    data_dict[\"Gender\"] = input(\"What is your gender?\")\n",
    "    data_dict[\"Score\"] = score\n",
    "\n",
    "    form_url = \"https://docs.google.com/forms/d/e/1FAIpQLSfI7JKtNgG8hwV3ypkuXPxx-7W75r2bbgWOjsm6rsx8aI4LCw/viewform?usp=sf_link\"    \n",
    "    send_to_google_form(data_dict, form_url)\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    memory_test()\n",
    "\n",
    "\n"
   ]
